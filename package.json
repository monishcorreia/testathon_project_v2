{
  "name": "browserstack-testathon-automate-playwright",
  "version": "0.2.0",
  "description": "Playwright tests for BrowserStack Testathon (Offers + Logout + Navigation).",
  "main": "index.js",
  "scripts": {
    "install-deps": "npm install",
    "test:local": "npx playwright test",
    "test:browserstack": "node scripts/run-on-browserstack.js",
